<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4"
      main_tree_to_execute="BehaviorTree">
  <BehaviorTree ID="BehaviorTree">
    <Sequence>
      <Delay delay_msec="1000">
        <SendGoal position_x="1"
                  position_y="2"
                  position_z="3"
                  orientation_x="0"
                  orientation_y="0"
                  orientation_z="0"
                  orientation_w="1"/>
      </Delay>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="SendGoal"
            editable="true">
      <input_port name="position_x"
                  default="0"/>
      <input_port name="position_y"
                  default="0"/>
      <input_port name="position_z"
                  default="0"/>
      <input_port name="orientation_x"
                  default="0"/>
      <input_port name="orientation_y"
                  default="0"/>
      <input_port name="orientation_z"
                  default="0"/>
      <input_port name="orientation_w"
                  default="1"/>
    </Action>
  </TreeNodesModel>

</root>
