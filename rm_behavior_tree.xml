<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4"
      main_tree_to_execute="BehaviorTree">
  <BehaviorTree ID="BehaviorTree">
    <Sequence>
      <Delay delay_msec="1000">
        <SendGoal linear_x="1"
                  linear_y="2"
                  linear_z="3"
                  angular_x="4"
                  angular_y="5"
                  angular_z="6"/>
      </Delay>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="SendGoal"
            editable="true">
      <input_port name="linear_x"
                  default="0"/>
      <input_port name="linear_y"
                  default="0"/>
      <input_port name="linear_z"
                  default="0"/>
      <input_port name="angular_x"
                  default="0"/>
      <input_port name="angular_y"
                  default="0"/>
      <input_port name="angular_z"
                  default="0"/>
    </Action>
  </TreeNodesModel>

</root>
